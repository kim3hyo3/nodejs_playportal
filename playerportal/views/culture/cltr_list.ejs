      </div>
      <div class="col-md-10">
        <!--iframe windows-->
        <!-- 16:9 aspect ratio -->
        <!--<div class="embed-responsive embed-responsive-16by9">
          <iframe class="embed-responsive-item" src="..."></iframe>
        </div>-->
        <div class="list-group">
          <li class="list-group-item active"><h4>cultureLifeCost_test</h4></li>
          <li class="list-group-item">
            <table class="table table-bordered">
              <tr>
                <th>cultureLifeCost_test</th>
                <th>grant</th>
                <th>use</th>
                <th>extinction</th>
                <th>balance</th>
              </tr>
              <%for(var i = 0; i < rows.length;i++){%>
              <tr>
                <td><%= rows[i].m_name %></td>
                <td><%= rows[i].grant %></td>
                <td><%= rows[i].use %></td>
                <td><%= rows[i].extinction %></td>
                <td><%= rows[i].balance %></td>
              </tr>
              <%}%>
              <!--<tr>
                <td>a</td>
                <td>b</td>
                <td>c</td>
                <td>d</td>
                <td>e</td>
              </tr>-->
            </table>
          </li>
        </div>
      </div>
    </div>
  </div>
